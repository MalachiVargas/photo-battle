<header  id ="header">
    
    <div id = "title" >Photo Battle</div>

</header>

<% @battles.each do |battle| %>
    <tr>
      <td><%= battle.id %></td>
      <td><%= link_to 'Show', battle_path(battle) %></td>
      <td><%= link_to 'Edit', edit_battle_path(battle) %></td>
      <td><%= link_to 'Destroy', battle_path(battle), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <br>
    </tr>
  <% end %>



